def insertion_sort(arr):
    n=len(arr)
    for i in range(1,n):
        v=arr(i)
        s=i-1
        while j>0 &arr[j]>v:
            arr[j+1]=arr[j]
            j-=1
            arr[j+1]=v
nums=input("Enter the elements separated by spaces:").spilt()
nums=[int(x)for x in nums]
print(f"Original list is :{nums}")
insertion_sort(nums)
print("Sorted list:",nums)
